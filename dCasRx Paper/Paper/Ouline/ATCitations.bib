  title         = {Fluoroquinolone-Gyrase-DNA Complexes: TWO MODES OF DRUG BINDING *},
  type          = {doi: 10.1074/jbc.M113.529164},
  url           = {https://doi.org/10.1074/jbc.M113.529164},
